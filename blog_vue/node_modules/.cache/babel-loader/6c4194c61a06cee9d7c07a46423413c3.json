N,GAAa,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,OAA3B;AACA,QAAA,KAAK,CAAC,WAAN,GAAoB,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,OAAlC;AACA,QAAA,KAAK,CAAC,KAAN,GAAc,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,KAA5B;AACA,QAAA,KAAK,CAAC,QAAN,GAAiB,GAAG,CAAC,IAAJ,CAAS,IAAT,CAAc,IAA/B;;AACA,QAAA,KAAK,CAAC,KAAN,CAAY,IAAZ,CAAiB,KAAK,CAAC,KAAvB;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,KAAlB,EAPuC,CAQtC;;AACD,QAAA,KAAK,CAAC,SAAN,GAAgB,KAAhB;AACD,OAVD;AAWD,KAfM;AAgBR,IAAA,YAhBQ,0BAgBM;AAAA;;AACX,UAAI,SAAQ,GAAI,KAAhB;;AACA,MAAA,MAAM,CAAC,QAAP,GAAkB,YAAM;AACxB;AACA,YAAI,cAAa,GAAI,QAAQ,CAAC,eAAT,CAAyB,YAAzB,GAAwC,QAAQ,CAAC,eAAT,CAAyB,SAAjE,GAA6E,MAAM,CAAC,WAApF,IAAmG,GAAxH;;AACA,YAAI,cAAa,IAAK,SAAQ,IAAK,KAAnC,EAA0C;AACxC,UAAA,SAAQ,GAAI,IAAZ,CADwC,CAExC;;AACA,UAAA,MAAI,CAAC,IAAL,CAAU,MAAI,CAAC,WAAL,EAAV;;AACA,UAAA,OAAO,CAAC,GAAR,CAAY,QAAM,QAAQ,CAAC,eAAT,CAAyB,SAA/B,GACX,OADW,GACH,MAAM,CAAC,WADJ,GACgB,QADhB,GACyB,QAAQ,CAAC,eAAT,CAAyB,YAD9D;AAEF;AACF,OAVE;AAWJ,KA7BS,CA8BT;AACA;AACA;;AAhCS;AAlBI,CAAf","sourcesContent":["<!--\r\n * @Description: \r\n * @Author: gaumn \r\n * @Date: 2022-02-06 09:55:08\r\n * @LastEditTime: 2022-02-12 15:09:44\r\n * @LastEditors: gaumn\r\n-->\r\n<template>\r\n\r\n  \r\n  <div class=\"blogs\">\r\n    <NavigationBar></NavigationBar>\r\n            首页\r\n    <div class=\"container\" ref=\"box\" >\r\n      <div v-for=\"array in shows\" :key=\"array\">\r\n        <div class=\"panel panel-default\" v-for=\"it in array\" :key=\"it\">\r\n           <div class=\"panel-body\">\r\n             {{it}}\r\n           </div>\r\n        </div>\r\n      </div>\r\n  \r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import NavigationBar from \"../components/NavigationBar.vue\";\r\n  import axios from \"axios\";\r\n  import moment from 'moment'\r\n  axios.defaults.baseURL = 'http://localhost:8081'\r\n  export default {\r\n    name: \"Blogs.vue\",\r\n    components: {NavigationBar},\r\n    data:function() {\r\n      return {\r\n        shows:[], //存放滚动区域要显示的数据\r\n        isLoading:false ,//默认没有在加载数据\r\n        blogs: {},\r\n        currentPage: 2,\r\n        total: 0,\r\n        pageSize: 5\r\n      }\r\n    },\r\n    mounted:function(){\r\n          this.page(1);\r\n          this.handleScroll();\r\n          // this.getRecommendList()\r\n    },\r\n    methods: {\r\n      page(currentPage) {\r\n        const _this = this;\r\n        this.isLoading=true;\r\n        axios.get(\"/\" + currentPage).then(res => {\r\n          console.log(res);\r\n          _this.blogs =res.data.data.records;\r\n          _this.currentPage = res.data.data.current;\r\n          _this.total = res.data.data.total;\r\n          _this.pageSize = res.data.data.size;\r\n          _this.shows.push(_this.blogs);\r\n          console.log(_this.shows);\r\n           //数据请求完成\r\n          _this.isLoading=false;\r\n        })\r\n      },\r\n     handleScroll(){\r\n        let isLoading = false\r\n        window.onscroll = () => {\r\n        // 距离底部200px时加载一次\r\n        let bottomOfWindow = document.documentElement.offsetHeight - document.documentElement.scrollTop - window.innerHeight <= 200\r\n        if (bottomOfWindow && isLoading == false) {\r\n          isLoading = true\r\n          //写后台加载数据的函数\r\n          this.page(this.currentPage++);\r\n          console.log(\"距顶部\"+document.documentElement.scrollTop\r\n          +\"可视区高度\"+window.innerHeight+\"滚动条总高度\"+document.documentElement.offsetHeight);\r\n        }\r\n      }\r\n    }\r\n    // created() {\r\n    //   this.page(1)\r\n    // }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n @import \"../css/PageBody.css\";\r\n  /* .mpage {\r\n    margin: 0 auto;\r\n    text-align: center;\r\n  } */\r\n\r\n</style>"],"sourceRoot":""}]}